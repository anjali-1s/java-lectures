// Write a java program to copying an array.
import java.lang.*;
public class Array_copy {
     public static void main(String arg[])
     {
          int A[] = {2,3,4,5,6,7,8};
          int B[] = new int[7];

          for(int i = 0; i < A.length; i++)
          {
              B[i] = A[i];
          }
           
          for(int y : A)
          System.out.print(y + " ");
          System.out.println(" ");

          for(int x : B)
          System.out.print(x + " ");
     }
}
