// Write a java program to demonastrate parameter passing in methods.

class Test
{
     static int max(int x, int y)
     {
          if(x > y)
             return x;
             else
                 return y;
     }

     public static void main(String arg[])
     {
          int a = 19, b = 45 , c;
          c = max(a,b);
          System.out.println(c);
          System.out.println(max(a,b));
     }
}
