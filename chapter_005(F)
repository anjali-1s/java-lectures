// Write a java program to demonstrate increment & decrement operator.

import java.lang.*;
class IncDec
{
    public static void main(String[] args)
{
   int x = 5, y = 4, z;
   ++x;
   System.out.println(x);
   y = x++;
   z = 2 * x++  +  3 * ++y;
   System.out.println(x + " " + y + " " + z);
   float a = 3.5f;
   a++;
   System.out.println(a);
   char ch = 'A';
   ch++;
   System.out.println(ch);
   byte b = 5;
   b++;
   System.out.println(b);
}
}
