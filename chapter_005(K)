// Write a java program to demonstrate casting.
import java.lang.*;
import java.util.Scanner;
class Printing
{
public static void main(String[] args)
{
   byte a = 9,b=12;
   byte c;
   c =(byte)(a<<4);
   System.out.println(c);
    c = (byte)(c|b);
    System.out.println(c);
    System.out.println((c & 0b11110000) >> 4);
    System.out.println((c & 0b00001111));
}
}

