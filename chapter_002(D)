// demonastrate literals
import java.lang.*;

class literals
{
     public static void main(String arg[])
{
       byte b = 10;
       long l = 23L;
       short s = 34;
       long L = 999_999_999;
       long lo = 9999999999L;

       System.out.println(lo);
       System.out.println(b);
       System.out.println(l);
       System.out.println(s);
       System.out.println(L);
}
}
