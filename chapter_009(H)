// Write a java program to rotate an array in left direction.
import java.lang.*;
public class Array_rotation {
    public static void main(String arg[])
    {
     int n = 10;
     int arr[] = {2,3,5,6,7,8,9,0};
     int temp = arr[0];
     for(int i = 1; i < arr.length; i++)
     {
          arr[i - 1] = arr[i];
     }
     arr[arr.length - 1] = temp;

     for(int x : arr)
     {
          System.out.print(x + " ");
     }

    } 
}
