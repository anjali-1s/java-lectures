// Write a java program to update values of an array through parameter passing.
class updation {

     static void update(int A[])
     {
        A[0] = 56;
        A[1] = 34;
     }
     
     public static void main(String arg[])
     {
          int A[] = {12,45,65,13};
          update(A);
          System.out.println(A[0]);
          System.out.println(A[1]);
     }
}
